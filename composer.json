{
    "name": "karelwintersky/composer-cleanup-plugin",
    "type": "composer-plugin",
    "description": "A composer cleanup plugin, to remove unnecessary files (tests, documentation, etc) to save space.",
    "license": "MIT",
    "authors": [
        {
            "name": "Karel Wintersky",
            "email": "karel.wintersky@gmail.com"
        }
    ],
    "version": "2.0",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KarelWintersky\\Composer\\": "src/"
        }
    },
    "extra": {
        "class": "KarelWintersky\\Composer\\Plugin"
    }
}
